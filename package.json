    "eigenvectors",
    "modularity"
  ],
  "author": "guinetik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guinetik/js-network-stats/issues"
  },
  "homepage": "https://github.com/guinetik/js-network-stats#readme",
  "dependencies": {
    "jsnetworkx": "^0.3.4"
  },
  "devDependencies": {
    "vite": "^4.4.9",
    "vitest": "^0.34.6",
    "@vitest/coverage-v8": "^0.34.6"
  }
}
