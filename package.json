{
  "name": "js-network-stats",
  "version": "1.5.0-rc1",
  "description": "A nodejs micro package to help calculating stats for network graphs",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "runner": "node ./src/index.runner.js",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guinetik/js-network-stats.git"
  },
  "keywords": [
